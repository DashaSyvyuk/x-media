{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "aaronddm/xml-builder": "^2.0",
        "beberlei/doctrineextensions": "^1.3",
        "composer/package-versions-deprecated": "*",
        "doctrine/doctrine-migrations-bundle": "*",
        "doctrine/orm": "*",
        "easycorp/easyadmin-bundle": "^4.0",
        "friendsofsymfony/ckeditor-bundle": "^2.4",
        "guzzlehttp/guzzle": "*",
        "jms/serializer-bundle": "*",
        "knplabs/knp-paginator-bundle": "*",
        "nesbot/carbon": "*",
        "phpdocumentor/reflection-docblock": "*",
        "phpoffice/phpspreadsheet": "^3.9",
        "ramsey/uuid": "*",
        "sensio/framework-extra-bundle": "*",
        "symfony/asset": "^7.3",
        "symfony/cache": "^7.3",
        "symfony/config": "^6.4",
        "symfony/console": "^7.3",
        "symfony/dependency-injection": "^6.4",
        "symfony/doctrine-bridge": "^7.3",
        "symfony/dotenv": "^7.3",
        "symfony/error-handler": "^6.4",
        "symfony/event-dispatcher": "^7.3",
        "symfony/expression-language": "^7.3",
        "symfony/filesystem": "^7.3",
        "symfony/flex": "^2.10",
        "symfony/form": "^7.3",
        "symfony/framework-bundle": "^6.4",
        "symfony/http-client": "^7.3",
        "symfony/http-foundation": "^7.3",
        "symfony/http-kernel": "^6.4",
        "symfony/intl": "^7.3",
        "symfony/mailer": "^7.3",
        "symfony/mime": "^7.3",
        "symfony/options-resolver": "^7.3",
        "symfony/process": "^7.3",
        "symfony/property-access": "^7.3",
        "symfony/property-info": "^7.3",
        "symfony/proxy-manager-bridge": "^6.4",
        "symfony/routing": "^7.3",
        "symfony/runtime": "^7.3",
        "symfony/security-bundle": "^7.2",
        "symfony/security-core": "^7.3",
        "symfony/security-csrf": "^7.3",
        "symfony/security-http": "^7.3",
        "symfony/string": "^7.3",
        "symfony/translation": "^7.3",
        "symfony/twig-bridge": "^6.4",
        "symfony/twig-bundle": "^6.4",
        "symfony/ux-twig-component": "^2.31",
        "symfony/validator": "^7.3",
        "symfony/web-link": "^7.3",
        "symfony/webpack-encore-bundle": "^2.3",
        "symfony/yaml": "^7.3",
        "twig/extra-bundle": "*",
        "twig/twig": "^3.0",
        "vich/uploader-bundle": "^2.1",
        "winzou/state-machine-bundle": "^0.6.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "audit": {
            "ignore": [
                "PKSA-wws7-mr54-jsny"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:warmup --no-optional-warmers": "symfony-cmd",
            "cache:clear --no-warmup --no-optional-warmers": "symfony-cmd",
            "ckeditor:install --clear=drop": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "squizlabs/php_codesniffer": "^4.0",
        "symfony/browser-kit": "^6.4",
        "symfony/css-selector": "^6.4",
        "symfony/debug-bundle": "^6.4",
        "symfony/dom-crawler": "^6.4",
        "symfony/maker-bundle": "^1.64",
        "symfony/phpunit-bridge": "^6.4",
        "symfony/stopwatch": "^6.4",
        "symfony/var-dumper": "^6.4",
        "symfony/web-profiler-bundle": "^6.4"
    }
}
